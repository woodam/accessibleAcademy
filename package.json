        "build:clean": "rimraf .next",
        "build:ui": "next build",
        "build:server": "tsc --project tsconfig.server.json && tsc-alias -p tsconfig.server.json && babel .next/production-server -d .next/production-server --extensions \".js\" && tsc-alias",
        "build": "npm run build:clean && npm run build:ui && npm run build:server",
        "start:test": "env-cmd -f .env.development cross-env NODE_ENV=development node .next/production-server/server/main.js",
        "start:stage": "env-cmd -f .env.stage cross-env NODE_ENV=stage node .next/production-server/server/main.js",
        "start:prod": "env-cmd -f .env.production cross-env NODE_ENV=production node .next/production-server/server/main.js",
        "start": "env-cmd -f .env.production cross-env NODE_ENV=production node .next/production-server/main.js",
        "test": "jest --config jest.config.js",
        "lint": "eslint",
        "lint:fix": "eslint --fix './src/**/*.{ts,tsx,js,jsx}'"
    },
